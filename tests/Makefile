AR := ar
CXX := g++

ifeq ($(debug), y)
	CXXFLAGS := -g
else
	CXXFLAGS := -O2 -DNDEBUG
endif
CXXFLAGS := $(CXXFLAGS) -Wall -Werror -Wextra -fPIC

TARGET := test_text_utils

.PHONY: all clean

all: $(TARGET)

.PHONY: __omake_dep__1

__omake_dep__1:
	$(MAKE) debug=$(debug) libtext_utils.a -C ..

test_text_utils_OBJS := test.cpp.test_text_utils.o

test_text_utils_INCLUDE := -I../..

test_text_utils_LIBS := ../libtext_utils.a

test.cpp.test_text_utils.o: test.cpp
	$(CXX) $(CXXFLAGS) $(test_text_utils_INCLUDE) -c $< -o $@

test_text_utils: $(test_text_utils_OBJS) | __omake_dep__1
	$(CXX) $(CXXFLAGS) -o $@ $^ $(test_text_utils_LIBS)

clean:
	rm -f $(TARGET) $(test_text_utils_OBJS)
